{
  "manifest_version": 3,
  "name": "Find whatever",
  "description": "Enhance your browser's find capabilities.Case recognition、Word recognition、regular expressions、Listen for DOM changes in real time",
  "version": "1.1.1",
  "icons": {
    "16": "popup.png",
    "32": "popup.png",
    "48": "popup.png",
    "128": "popup.png"
  },
  "author": "2223133607@qq.com",
  "action": {
    "default_icon": {
      "16": "popup.png",
      "32": "popup.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting", "webNavigation", "tabs"],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": ["contentScript.bundle.js"],
      "all_frames": true
    }
  ],
  "background": { "service_worker": "background.bundle.js" },
  "externally_connectable": {
    "matches": ["*://*/*"]
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+F",
        "mac": "Command+F"
      }
    }
  },
  "web_accessible_resources": [
    {
      "resources": ["icon-128.png", "icon-34.png"],
      "matches": []
    }
  ]
}
