{
  "name": "Find whatever",
  "description": "Enhance your browser's find capabilities.Case recognition、Word recognition、regular expressions、Listen for DOM changes in real time",
  "version": "0.0.5",
  "manifest_version": 3,
  "icons": {
    "16": "popup.png",
    "32": "popup.png",
    "48": "popup.png",
    "128": "popup.png"
  },
  "author": "2223133607@qq.com",
  "action": {
    "default_icon": {
      "16": "popup.png",
      "32": "popup.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting", "webNavigation", "tabs"],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["popContent.js"],
      "all_frames": true
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["*://*/*"]
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+F",
        "mac": "Alt+F"
      }
    }
  }
}
