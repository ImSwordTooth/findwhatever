{
  "name": "Find whatever",
  "description": "Find whatever",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_icon": {
      "16": "popup.png",
      "32": "popup.png"
    }
  },
  "permissions": ["storage", "declarativeContent", "activeTab", "scripting"],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["style.css"],
      "js": ["popContent.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "externally_connectable": {
    "matches": ["*://*/*"]
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+F",
        "mac": "Alt+F"
      }
    }
  }
}
